<div class="userSettingsHeader">
    <h1 class="userSettingsHeader1">{{lang.userCalendarHeader}}</h1>
</div>
<form>
    <div class="userSettingsCalendar">
        <h3>{{lang.calendarUrl}}</h3>
        <div class="shift">{{calendarUrl}}</div>

        <h3>{{lang.calendarFrequency}}</h3>
        <div class="calendarFrequencies">
            <div class="calendarFrequency"><input type="radio" name="frequency">{{lang.upTo3events}}</div>
            <div class="calendarFrequency"><input type="radio" name="frequency">{{lang.upTo6events}}</div>
            <div class="calendarFrequency"><input type="radio" name="frequency">{{lang.upTo10events}}</div>
            <div class="calendarFrequency"><input type="radio" name="frequency">{{lang.upTo15events}}</div>
            <div class="calendarFrequency"><input type="radio" name="frequency">{{lang.upTo20events}}</div>
        </div>

    </div>
</form>
